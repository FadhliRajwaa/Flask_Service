# Aktifkan mode simulasi untuk deployment di lingkungan dengan memori terbatas
# 0 = Coba pakai model asli, 1 = Paksakan mode simulasi
SIMULATION_MODE=0

# Paksa penggunaan model bahkan jika SIMULATION_MODE=1 
# (ubah ke 1 untuk memaksa menggunakan model)
FORCE_MODEL=1

# Batasi penggunaan memori TensorFlow
TF_FORCE_GPU_ALLOW_GROWTH=true
TF_CPP_MIN_LOG_LEVEL=2

# Port untuk deployment
PORT=10000